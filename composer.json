{
    "name": "mxenabled/atrium-php",
    "description": "A PHP wrapper for the MX API.",
    "keywords": [
        "php",
        "sdk",
        "api"
    ],
    "homepage": "http://atrium.mx.com",
    "license": "MIT",
    "authors": [
        {
            "name": "MX",
            "homepage": "https://atrium.mx.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~2.12"
    },
    "autoload": {
        "psr-4": { "atrium\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "atrium\\" : "test/" }
    }
}
